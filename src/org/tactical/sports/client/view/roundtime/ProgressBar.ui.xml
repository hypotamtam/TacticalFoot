<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:LayoutPanel ui:field="m_mainPanel">
		<g:layer left="0.0pct" width="100.0pct" top="0.0pct" height="100.0pct">
			<g:SimplePanel ui:field="m_background">
				<g:Image width="100%" height="100%" ui:field="m_bgImage" visible="false"/>
			</g:SimplePanel>
		</g:layer>
		<g:layer width="100.0pct" height="100.0pct" bottom="0.0pct" left="0.0pct">
			<g:SimplePanel ui:field="m_foreground">
				<g:Image width="100%" height="100%" ui:field="m_fgImage" visible="false"/>
			</g:SimplePanel>
		</g:layer>
		
	</g:LayoutPanel>
</ui:UiBinder> 